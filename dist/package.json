{
  "name": "@modern-mean/angular-base",
  "version": "0.0.12",
  "description": "angular base",
  "main": "bundles/@modern-mean/angular-base.umd.js",
  "private": false,
  "scripts": {
    "test": "npm test",
    "postversion": "git push && git push --tags && npm publish --access public",
