  "license": "ISC",
  "bugs": {
    "url": "https://github.com/modern-mean/angular-base/issues"
  },
  "homepage": "https://github.com/modern-mean/angular-base#readme",
  "dependencies": {
    "@angular/flex-layout": "^2.0.0-beta.1",
    "@angular/material": "^2.0.0-alpha.11",
    "hammerjs": "^2.0.8"
  },
  "peerDependencies": {
    "@angular/common": "^2.2.4",
    "@angular/core": "^2.2.4",
    "rxjs": "^5.0.0-rc.4"
  },
  "module": "./index.js",
  "typings": "index.d.ts"
